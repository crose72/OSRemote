# Flutter / Dart formatting
root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true
indent_style = space
indent_size = 2

# Make sure dart format rules are respected
[*.dart]
indent_style = space
indent_size = 2
max_line_length = 100
insert_final_newline = true
